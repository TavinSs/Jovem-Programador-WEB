<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Jovem_Programador_WEB</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Jovem_Programador_WEB.styles.css" asp-append-version="true" />
    <style>
        body {
            background-color: #212121;
        }

        h1 {
            color: gainsboro;
        }

        div {
            color: Black;
        }

        th {
            color: gainsboro;
        }

        td {
            color: gainsboro;
        }

        #Nav {
            color: #690420;
        }

        #Navs {
            color: gainsboro;
        }

        #linha {
            padding: 100px 10px;
        }

        nav {
            background-color: rgb(59,59,59)
        }
    </style>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg bg-body-tertiary">
            <div class="container-fluid">
                <a id="Nav" class="navbar-brand" href="#">Home</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                    @* <div class="navbar-nav">
                        <a id="Navs" class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Pagina inicial</a>
                        <a id="Navs" class="nav-link " asp-area="" asp-controller="Aluno" asp-action="Aluno">Alunos</a>
                        <a id="Navs" class="nav-link " asp-area="" asp-controller="Professor" asp-action="Professor">Professor</a>
                    </div> *@
                </div>
            </div>
        </nav>

        <img scr="https://i.etsystatic.com/14951240/r/il/6b7a35/3536263578/il_794xN.3536263578_8ry6.jpg">
    </header>

    <div id="linha" class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
